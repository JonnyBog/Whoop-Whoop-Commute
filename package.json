{
  "name": "whoop-whoop-commute",
  "version": "1.0.0",
  "description": "Whoop Whoop Commute",
  "main": "server/server.jsx",
  "scripts": {
    "test": "BABEL_ENV=test jest --config jest.config.json --watch",
    "test-ci": "BABEL_ENV=test jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --watch --coverage",
    "coverage-ci": "jest --config jest.config.json --coverage || true",
    "eslint": "eslint . --ext .js,.jsx",
    "eslint-ci": "eslint . --ext .js,.jsx --format jslint-xml -o ./test-reports/unit-test-reports/eslint.xml",
    "stylelint": "stylelint . || echo \"lint failed for some file(s).\"",
    "watch-app": "BABEL_ENV=app NODE_ENV=development webpack --config webpack.config.js --watch --mode development",
    "watch-server": "NODE_ENV=development PORT=3000 browser-refresh --inspect --harmony-promise-finally static/server.js",
    "start-prod": "BABEL_ENV=app NODE_ENV=production npm run build-prod && pm2-docker ecosystem.config.js",
    "build-prod": "webpack --config webpack.config.js --mode production",
    "stop-prod": "pm2 stop ecosystem.config.js",
    "restart-prod": "pm2 restart ecosystem.config.js"
  },
  "jest-junit": {
    "output": "./test-reports/unit-test-reports/junit/junit.xml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://JonnyBoggon@bitbucket.org/JonnyBoggon/whoop-whoop-commute.git"
  },
  "author": "Jonny Boggon",
  "homepage": "https://bitbucket.org/JonnyBoggon/whoop-whoop-commute#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-refresh": "^1.7.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^23.1.0",
    "jest-junit": "^5.0.0",
    "jest-styled-components": "^5.0.1",
    "raw-loader": "^0.5.1",
